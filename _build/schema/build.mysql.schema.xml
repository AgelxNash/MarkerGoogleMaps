<!-- The following xPDO model represents an object-relational map structure of StoreLocator -->
<model package="storelocator" baseClass="xPDOObject" platform="mysql" defaultEngine="MyISAM" phpdoc-package="" phpdoc-subpackage="">
    <object class="slStore" table="storelocator_stores" extends="xPDOSimpleObject">
		<field key="description" dbtype="varchar" precision="150" phptype="string" null="false" default="" />
		<field key="latitude" dbtype="decimal" precision="9,6" phptype="float" null="false" default="" />
		<field key="longitude" dbtype="decimal" precision="9,6" phptype="float" null="false" default="" />
		<field key="resource_id" dbtype="int" precision="10" phptype="integer" null="false" default="" />
		<field key="sort" dbtype="int" precision="10" phptype="integer" null="false" default="" />
		<field key="config" dbtype="text" phptype="json" null="false" default="" />
	</object>
</model>